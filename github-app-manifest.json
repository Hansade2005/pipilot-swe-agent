{
  "name": "PiPilot SWE Agent",
  "description": "Autonomous software engineering agent that can read, write, and manage repository code through GitHub API",
  "url": "https://github.com/Hansade2005/pipilot-swe-agent",
  "hook_attributes": {
    "url": "https://your-domain.com/api/webhooks/github",
    "active": true
  },
  "redirect_url": "https://github.com/apps/pipilot-swe-agent/installations/new",
  "public": false,
  "default_permissions": {
    "contents": "read",
    "issues": "write",
    "pull_requests": "write",
    "repository_hooks": "write",
    "statuses": "read",
    "workflows": "write",
    "actions": "read",
    "checks": "read",
    "metadata": "read",
    "members": "read"
  },
  "default_events": [
    "issues",
    "issue_comment",
    "pull_request",
    "pull_request_review",
    "pull_request_review_comment",
    "push",
    "workflow_run",
    "installation",
    "installation_repositories"
  ]
}